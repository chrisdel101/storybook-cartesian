{
  "name": "storybook-cartesian",
  "homepage": "https://github.com/jondot/storybook-cartesian",
  "keywords": [
    "storybook",
    "testing",
    "react"
  ],
  "version": "1.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc -w",
    "watch": "jest --watch",
    "test": "jest",
    "toc": "doctoc README.md",
    "release": "yarn build && git commit -am 'build'; yarn version && git push --tags && git push origin master",
    "build": "tsc"
  },
  "dependencies": {
    "flat": "^4.1.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/flat": "^0.0.28",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "doctoc": "^1.4.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}